  "scripts": {
    "mkdir-ui": "mkdir -p ./dist/libs && mkdir -p ./dist/assets && mkdir -p ./build/javascript",
    "compile": "tsc -b",
    "copy-core": "cp node_modules/@swim/core/dist/umd/*.min.js* dist/libs",
    "copy-gui": "cp node_modules/@nstream/gui/dist/umd/*.min.js* dist/libs",
    "copy-host": "cp node_modules/@swim/host/dist/umd/*.min.js* dist/libs",
    "copy-maps": "cp node_modules/@swim/maps/dist/umd/*.min.js* dist/libs",
    "copy-ui": "cp node_modules/@swim/ui/dist/umd/*.min.js* dist/libs",
    "copy-ux": "cp node_modules/@swim/ux/dist/umd/*.min.js* dist/libs",
    "copy-vis": "cp node_modules/@swim/vis/dist/umd/*.min.js* dist/libs",
    "copy-libs": "npm run mkdir-ui && npm run copy-core && npm run copy-host && npm run copy-ui && npm run copy-ux && npm run copy-vis && npm run copy-maps && npm run copy-gui",
    "copy-app": "npm run copy-libs && cp ./*.html ./dist && cp -rf ./assets/* ./dist/assets && cp -rf ./build/javascript/* ./dist/libs",
    "bundle": "cd ./src && rollup -c && npm run copy-app",
    "build": "npm run compile && npm run bundle",
    "serve": "cd ../server && mvn compile exec:java -Dexec.mainClass=\"io.nstream.demos.cruise.EntryPoint\"",
    "dev": "rm -rf ../server/src/main/resources/ui && npm run build && cp -r ./dist/ ../server/src/main/resources/ui && npm run serve"
  }
